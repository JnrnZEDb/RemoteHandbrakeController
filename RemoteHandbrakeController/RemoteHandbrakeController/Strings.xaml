<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:RemoteHandbrakeController">

	<system:String x:Key="TestIPToolTip">Tests the connection to the IP (10s timeout).</system:String>
	<system:String x:Key="LinuxInputToolTip">Media file input directory from Linux Server point of view.</system:String>
	<system:String x:Key="LinuxOutputDirectoryToolTip">Media file output directory.</system:String>
	<system:String x:Key="TestModeToolTip">Will do a ping command instead of encoding for simple testing</system:String>
	<system:String x:Key="CustomHandbrakePresets">Directory which contains any Custom Handbrake Presets (.json files)</system:String>
	  
</ResourceDictionary>